{
    "$schema": "vendor/infection/infection/resources/schema.json",
    "source": {
        "directories": [
            "src/Downloader",
            "src/Exception"
        ],
        "excludes": [
            "CurlTransport.php"
        ]
    },
    "logs": {
        "text": "infection.log"
    },
    "mutators": {
        "@default": true,
        "Throw_": {
            "ignore": [
                "Liquetsoft\\Fias\\Component\\Downloader\\CurlDownloader::openLocalFile"
            ]
        },
        "FunctionCallRemoval": {
            "ignore": [
                "Liquetsoft\\Fias\\Component\\Downloader\\CurlDownloader"
            ]
        },
        "MethodCallRemoval": {
            "ignore": [
                "Liquetsoft\\Fias\\Component\\Downloader\\CurlDownloader"
            ]
        }
    }
}