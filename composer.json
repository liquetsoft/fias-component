{
    "name": "liquetsoft/fias-component",
    "description": "FIAS database parser for php.",
    "type": "library",
    "keywords": ["php", "fias"],
    "license": "MIT",
    "require": {
        "ext-soap": "*",
        "ext-libxml": "*",
        "ext-xmlreader": "*",
        "php": ">=7.2",
        "symfony/yaml": "^4.2",
        "symfony/serializer": "^4.2",
        "symfony/property-access": "^4.2",
        "symfony/property-info": "^4.2",
        "psr/log": "^1.1",
        "ramsey/uuid": "^3.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "fzaninotto/faker": "^1.7",
        "friendsofphp/php-cs-fixer": "^2.16",
        "sebastian/phpcpd": "^4.0",
        "vimeo/psalm": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Liquetsoft\\Fias\\Component\\": "src/",
            "Liquetsoft\\Fias\\Component\\Tests\\": "tests/src"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/liquetsoft/fias-component"
        }
    ]
}
